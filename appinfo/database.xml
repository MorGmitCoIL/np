<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>

 <name>*dbname*</name>
 <create>true</create>
 <overwrite>false</overwrite>

 <charset>utf8</charset>

 <table>

  <name>*dbprefix*file_data</name>

  <declaration>


   <field>
    <name>file_path</name>
    <type>text</type>
    <default></default>
    <notnull>true</notnull>
    <length>255</length>
   </field>

  <field>
    <name>json_file_data</name>
    <type>text</type>
    <default></default>
    <notnull>true</notnull>
    <length>255</length>
   </field>
  </declaration>

 </table>



</database>
